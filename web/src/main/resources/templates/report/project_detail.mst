<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=1280" />
    <title>项目详情报告</title>
  </head>
  <body>
    <div class="container">
      <div>
        <div class="charts" id="people-charts"></div>
      </div>

      <div>
        <div class="charts" id="task-type-pie"></div>
        <div class="charts" id="task-type-pie-num"></div>
      </div>


    </div>

    <script src="https://code.highcharts.com/highcharts.js"></script>
    <script src="/public/scripts/projectDetail.js"></script>

    <script charset="utf-8">
      var taskTypStat = [];
      {{#taskStats}}
        taskTypStat.push({name: '{{taskTyp}}', value: {{consumedTime}} })
      {{/taskStats}}
      renderPie('task-type-pie', '任务类型耗时报表', taskTypStat);

      var taskTypStatNum = [];
      {{#taskStats}}
        taskTypStatNum.push({name: '{{taskTyp}}', value: {{totalNum}} })
      {{/taskStats}}
      renderPie('task-type-pie-num', '任务类型任务数报表', taskTypStatNum);
    </script>
  </body>
</html>
